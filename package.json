{
  "name": "concurrent-user-processing-service",
  "version": "1.0.0",
  "description": "micro-service controlling concurrent user access to content",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "test": "mocha -r ts-node/register src/**/*.test.ts",
    "test:watch": "mocha --require ts-node/register --watch --watch-files src, src/**/*.test.ts",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "concurrency",
    "user",
    "control",
    "device",
    "limit"
  ],
  "author": "Jameel",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.5",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "chai": "^4.3.6",
    "concurrently": "^7.2.2",
    "eslint": "^8.20.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/ramda": "^0.28.15",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongodb": "^4.8.0",
    "ramda": "^0.28.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  }
}
